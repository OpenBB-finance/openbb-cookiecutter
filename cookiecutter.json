    "full_name": "Enter your full name",
    "email": "Enter your email",
    "project_name": "Enter the extension name",
    "project_tag": "Enter the project tag",
    "package_name": "Enter the package name",
    "add_provider": {
      "__prompt__": "Will your extension retrieve data from an API, a database or a file?",
      "yes": "Yes",
      "no": "No"
    },
    "add_command": {
      "__prompt__": "Will your extension add a new command to obb?",
      "yes": "Yes",
      "no": "No"
    },
    "process_data": {
      "__prompt__": "Will your extension process data provided by the user?",
      "yes": "Yes",
      "no": "No"
    }
  }
